/boot		emmc		/dev/block/mmcblk0p20                           flags=backup=1;display="boot"
/recovery	emmc		/dev/block/mmcblk0p21                           flags=backup=1;display="recovery"
/data		ext4		/dev/block/mmcblk0p27                           length=-16384;flags=backup=1;display="data"
/system		ext4		/dev/block/mmcblk0p25                           flags=backup=1;display="system"
/cache		ext4		/dev/block/mmcblk0p24                           flags=backup=1;display="cache"
/efs		emmc		/dev/block/mmcblk0p17                           flags=backup=1;display="EFS"
/modem		emmc		/dev/block/mmcblk0p8                           flags=backup=1;display="Modem"
/modem2		emmc		/dev/block/mmcblk0p9                           flags=backup=1;display="Modem2"
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="extSdCard";storage;wipeingui;removable
/usb-otg	auto		/dev/block/sda1                           display="usbotg"
